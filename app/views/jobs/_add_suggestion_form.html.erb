<%= simple_form_for(new_job) do |f| %>
  <%= f.hidden_field :title, value: job[:title] %>
  <%= f.hidden_field :company, value: job[:company] %>
  <%= f.hidden_field :url, value: job[:url] %>
   <%= f.hidden_field :logo_url, value: job[:logo_url] %>
  <%= f.submit 'Add to jobs', class: 'btn btn-sm btn-primary add-job-btn' %>
<% end %>